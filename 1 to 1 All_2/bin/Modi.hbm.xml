<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="Com.Modi" table="Modi">
		<id name="id" >
			<generator class="assigend">
				<param name="property">Party</param>
				</generator>
		</id>
		
		<property name="name" />
		
		<one-to-one name="party" class="Com.BJP" cascade="all" />

	
		
	</class>
</hibernate-mapping>