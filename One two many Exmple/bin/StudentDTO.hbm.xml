<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping> 
   <class name="Com.Student" table="Studen">  
        
<id name="sid" column="sid"/>
<property name="firstname" column="firstname" length="10"/>
<property name="lastname" column="lastname" length="10"/>

<property name="fid" column="fid" insert="false"/>

    </class> 
</hibernate-mapping>